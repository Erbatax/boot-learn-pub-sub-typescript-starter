{
  "compilerOptions": {
    "target": "es2022",
    "lib": ["es2022", "dom", "dom.Iterable"],
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "allowJs": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true,
    "verbatimModuleSyntax": true
  }
}
